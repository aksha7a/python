my_set = {1,2,2,3,4,4,4}
print("Set :", my_set)
my_set.add(5)
print("Updated set:", my_set)
set1 = my_set
set2 = {2,4,4,6}
print("\nSet 1", set1)
print("Set 2", set2)
print("Difference")
print(set.difference(set2))
print("Symmeteric difference")
print(set1.symmetric_difference(set2))
print("union")
print(set1.union(set2))
print("intersection")
print(set1.intersection(set2))
