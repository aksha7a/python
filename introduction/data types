name = "Akshara"
age = 17
is_student = True
weight = 50
print("Name:" , name)
print("Data type of name is:" , type(name))
print("Age:" , 17) 
print("Data type of age is:" , type(age))
print("is_student:" , True)
print("Data type of is_student is:" , type(is_student))
print("Weight:" , 50)
print("Data type of weight is:" , type(weight))
